{
    "name": "overloop-testing-framework-frontend",
    "version": "0.1.0",
    "private": true,
    "proxy": "https://otf.overloop.io",
    "dependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.11",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.10",
        "babel-plugin-relay": "^14.1.0",
        "classnames": "^2.3.2",
        "react": "^18.2.0",
        "react-bootstrap-icons": "^1.10.2",
        "react-dom": "^18.2.0",
        "react-relay": "^14.1.0",
        "react-scripts": "5.0.1",
        "react-syntax-highlighter": "^15.5.0",
        "relay-runtime": "^14.1.0",
        "sass": "^1.57.1",
        "typescript": "^4.9.4",
        "use-debounce": "^9.0.3",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint:css": "./node_modules/.bin/stylelint '**/*.scss'",
        "lint:js": "./node_modules/.bin/eslint 'src/**/*.ts' 'src/**/*.tsx'",
        "lint": "npm run lint:js && npm run lint:css",
        "lint:fix": "npm run lint:js -- --fix && npm run lint:css -- --fix",
        "gql:schema": "graphql-codegen --config ./gql.codegen.ts",
        "gql:build": "npm run gql:schema && relay-compiler",
        "prettier": "npx prettier src --check",
        "prettier:fix": "npm run prettier -- --write"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@graphql-codegen/cli": "3.0.0",
        "@graphql-codegen/schema-ast": "^3.0.0",
        "@types/react-relay": "^14.1.3",
        "@types/react-syntax-highlighter": "^15.5.6",
        "@types/relay-runtime": "^14.1.6",
        "@typescript-eslint/eslint-plugin": "^5.53.0",
        "@typescript-eslint/parser": "^5.53.0",
        "eslint": "^8.33.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "graphql": "^16.6.0",
        "jsdom": "^21.1.0",
        "prettier": "^2.8.4",
        "relay-compiler": "^14.1.0",
        "stylelint": "^14.16.1",
        "stylelint-config-standard-scss": "^6.1.0"
    }
}
